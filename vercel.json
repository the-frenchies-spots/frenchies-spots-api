{
  "version": 2,
  "buildCommand": "npx prisma generate && nest build",
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node",
      "config": { "usePM": "pnpm" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    }
  ]
}
